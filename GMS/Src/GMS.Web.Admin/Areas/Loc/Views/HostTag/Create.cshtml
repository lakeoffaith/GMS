@model GMS.Loc.Contract.Host

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_LayoutOld.Edit.cshtml";
}

@{
    Layout = "~/Views/Shared/_LayoutOld.Edit.cshtml";
}
@section PageSpecificStyleSheetIncludes{
    <style type="text/css">
        .controls label
        {
            display:inline;
        }
    </style>
}
@section PageSpecificJavascriptIncludes{
    <script type="text/javascript">
        $("#BirthDate").datepicker();
   </script>
}
@section MainContent{
<div class="portlet-body form-horizontal form-bordered form-row-stripped">
    <div class="row-fluid"> 
            <div class="alert alert-info">
                <button class="close" data-dismiss="alert"></button>
                分配到部门请到<strong>“组织结构管理”</strong>菜单操作！
            </div>

            <div class="control-group">
                <label class="control-label"><span class="required">*</span>姓名：</label>
                <div class="controls">
                @Html.TextBoxFor(m => m.HostName, new { @class = "m-wrap" })
                <span class="help-inline">@Html.ValidationMessageFor(m => m.HostName)</span>
                </div>
            </div>

            
    </div>
</div>
}

